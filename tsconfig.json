{
  "compilerOptions": {
  "target": "ES2020",
  "useDefineForClassFields": true,
  "lib": ["DOM", "ES2021"],
  "jsx": "react-jsx",
  "module": "ESNext",
  "moduleResolution": "Node",
  "resolveJsonModule": true,
  "isolatedModules": true,
  "esModuleInterop": true,
  "skipLibCheck": true,
  "strict": true,
  "forceConsistentCasingInFileNames": true,
  "baseUrl": "src"
  },
  "include": ["src"]
}
